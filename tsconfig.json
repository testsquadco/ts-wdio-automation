{
  "compilerOptions": {
    "target": "es2019",
    "types": [
      "node",
      "@wdio/globals",
      "@wdio/mocha-framework",
      "@wdio/appium-service",
      "expect-webdriverio"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
} 